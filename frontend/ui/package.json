{
	"name": "@erynoa/ui",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"svelte": "./src/index.ts",
	"main": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"svelte": "./src/index.ts",
			"default": "./src/index.ts"
		},
		"./components/*": {
			"types": "./src/components/*/index.ts",
			"svelte": "./src/components/*/index.ts",
			"default": "./src/components/*/index.ts"
		},
		"./utils": {
			"types": "./src/utils.ts",
			"default": "./src/utils.ts"
		},
		"./styles": {
			"default": "./src/styles/index.css"
		},
		"./styles/tokens": {
			"default": "./src/styles/tokens.css"
		}
	},
	"scripts": {
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"lint": "biome check src/",
		"lint:fix": "biome check --write src/"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@lucide/svelte": "^0.561.0",
		"@sveltejs/kit": "^2.50.1",
		"bits-ui": "^2.15.4",
		"clsx": "^2.1.1",
		"lucide-svelte": "^0.563.0",
		"mode-watcher": "^1.1.0",
		"svelte": "^5.48.2",
		"svelte-check": "^4.3.5",
		"svelte-sonner": "^1.0.7",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"typescript": "^5.9.3"
	}
}
